{
  "name": "rsp-client",
  "version": "0.0.2",
  "description": "Runtime Server Protocol Client",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "build": "tslint -p tslint.json && tsc",
    "update-deps": "node_modules/.bin/ncu --upgrade --loglevel verbose --packageFile package.json && npm update",
    "build-ts": "rm -rf lib && tsc",
    "test-spec": "mocha lib/test/**/*test.js -R spec --bail",
    "test-cov": "istanbul cover --include-all-sources --dir ./coverage node_modules/mocha/bin/_mocha -- lib/test/**/*test.js -R spec --bail",
    "test-remap": "remap-istanbul -i ./coverage/coverage.json -o coverage -t html",
    "test-build": "npm run build-ts",
    "prepublish": "npm run build && tsc",
    "test": "npm run test-build && npm run test-cov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redhat-developer/rsp-client.git"
  },
  "author": "Red Hat Developers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redhat-developer/rsp-client/issues"
  },
  "homepage": "https://github.com/redhat-developer/rsp-client#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.2",
    "@types/sinon": "^5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "sinon": "^6.1.3",
    "sinon-chai": "^3.2.0",
    "source-map-support": "^0.5.6",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-no-unused-expression-chai": "^0.1.3",
    "typescript": "^2.9.2",
    "vscode-jsonrpc": "^3.6.2"
  },
  "dependencies": {
    "istanbul": "^0.4.5",
    "node-delete": "^0.1.2",
    "remap-istanbul": "^0.11.1"
  }
}
